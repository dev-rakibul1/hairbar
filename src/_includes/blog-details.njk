---
title: blog
layout: "base.njk"
---

<!-- Start page-wrap -->
<div class="page-wrap">
  {%-for news in collections.news-%}
  <!--======================================
        START HEADER
 ======================================-->
  <div class="body-overlay"></div>
  <div class="full-screen-search-container">
    <a class="full-screen-search-cancel icon-element icon-element-md" href="#">
      <i class="fal fa-times"></i>
    </a>
    <div class="full-screen-search">
      <div class="container">
        <div class="row">
          <div class="col-lg-8 mx-auto">
            <form class="form-search">
              <input type="text" class="form-control" placeholder="Search.." />
              <button class="btn" type="button">
                <i class="fal fa-search"></i>
              </button>
            </form>
          </div>
        </div>
      </div>
    </div>
    <!-- end full-screen-search -->
  </div>
  <!--======================================
        END HEADER
======================================-->

  <!--======================================
        START HERO
======================================-->
  <section class="breadcrumb-section bg-primary-100">
    <div class="container">
      <div class="breadcrumb-content text-center">
        <h1 class="bread-title mb-3">{{ news.data.title }}</h1>
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb breadcrumb-s1 justify-content-center">
            <li class="breadcrumb-item"><a href="#">Home</a></li>
            <li class="breadcrumb-item"><a href="#">Blog</a></li>
            <li class="breadcrumb-item active" aria-current="page">
              {{ news.data.title }}
            </li>
          </ol>
        </nav>
      </div>
    </div>
    <!-- end container -->
  </section>
  <!-- end breadcrumb-section -->
  <!--======================================
        END HERO
======================================-->

  <!--======================================
        START BLOG
======================================-->
  <section class="blog-section section-space">
    <div class="container">
      <div class="row">
        <div class="col-lg-8">
          <div class="single-entry mb-5 mb-lg-0">
            <img
              src="{{ news.data.featureImage }}"
              alt="blog image"
              class="card-img mb-3"
            />
            <p class="mb-2">
              {{ news.data.blogTime }}
              <a href="#" class="btn-link mr-2">{{ news.data.author }}</a> in
              <a href="#" class="btn-link mr-2">{{ news.data.catagories }}</a>
              <a href="#comments" class="btn-link">{{ news.data.comment }}</a>
            </p>
            <p class="mb-3">
              {{ news.data.blogPara1 }}
            </p>
            <p class="mb-3">
              {{ news.data.blogPara2 }}
            </p>
            <blockquote class="blockquote mb-4">
              <p class="mb-3">
                {{ news.data.blockquotePara }}
              </p>
              <footer class="blockquote-footer">
                {{ news.data.blockquotePerson }}

                <cite title="Source Title">
                  {{ news.data.blockquotePersonTitle }}</cite
                >
              </footer>
            </blockquote>

            <p class="mb-4">
              {{ news.data.blogPara3 }}
            </p>
            <div class="row align-items-center mb-4">
              <div class="col-lg-6 col-md-6">
                <img
                  src="{{ news.data.blogImage1 }}"
                  alt="single case image"
                  class="w-100 rounded mb-4 mb-lg-0"
                />
              </div>
              <!-- end col-lg-6 -->
              <div class="col-lg-6 col-md-6">
                <ul class="list-item">
                  <li>
                    <i class="far fa-check mr-2 text-success"></i
                    >{{ news.data.featureItems1 }}
                  </li>
                  <li>
                    <i class="far fa-check mr-2 text-success"></i>
                    {{ news.data.featureItems2 }}
                  </li>
                  <li>
                    <i class="far fa-check mr-2 text-success"></i>
                    {{ news.data.featureItems3 }}
                  </li>
                  <li>
                    <i class="far fa-check mr-2 text-success"></i>
                    {{ news.data.featureItems4 }}
                  </li>
                  <li>
                    <i class="far fa-check mr-2 text-success"></i>
                    {{ news.data.featureItems5 }}
                  </li>
                  <li>
                    <i class="far fa-check mr-2 text-success"></i>
                    {{ news.data.featureItems1 }}
                  </li>
                  <li>
                    <i class="far fa-check mr-2 text-success"></i>
                    {{ news.data.featureItems4 }}
                  </li>
                </ul>
              </div>
              <!-- end col-lg-6 -->
            </div>
            <p>
              {{ news.data.blogPara4 }}
            </p>
            <hr />
            <div
              class="d-flex flex-wrap align-items-center justify-content-between"
            >
              <ul class="tags my-1">
                <li><strong>Tags:</strong></li>
                <li>
                  <a href="#">{{ news.data.catagoriesTags1 }}</a>
                </li>
                <li>
                  <a href="#">{{ news.data.catagoriesTags2 }}</a>
                </li>
              </ul>
              <ul class="social-profile-icons social-profile-icons-s2 my-1">
                <li><strong>Share:</strong></li>
                <li>
                  <a href="{{ news.data.fa }}"
                    ><i class="fab fa-facebook-f"></i
                  ></a>
                </li>
                <li>
                  <a href="{{ news.data.tw }}"
                    ><i class="fab fa-twitter"></i
                  ></a>
                </li>
                <li>
                  <a href="{{ news.data.inst }}"
                    ><i class="fab fa-instagram"></i
                  ></a>
                </li>
              </ul>
            </div>
            <!-- end d-flex -->
            <hr />
            <ul class="posts-nav row my-5">
              <li class="prev-post col-lg-6 text-left">
                <a href="#"
                  ><span>{{ news.data.slideBtnPrev }}</span>
                  {{ news.data.slideBtnTitle1 }}
                </a>
              </li>
              <li class="next-post col-lg-6 text-right">
                <a href="#"
                  ><span>{{ news.data.slideBtnNext }}</span>
                  {{ news.data.slideBtnTitle2 }}
                </a>
              </li>
            </ul>
          </div>
          <!-- end single-entry -->

          <div class="entry-author-box mt-5">
            <div class="author-img">
              <img src="path-to-author-image.jpg" alt="author image" />
            </div>
            <div class="author-content">
              <h4 class="author-name">John Doe</h4>
              <p class="author-info">
                Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do
                eiusmod tempor incididunt ut labore et dolore magna aliqua.
              </p>
            </div>
          </div>
          <!-- end entry-author-box -->

          <div class="comment-area mt-5">
            <h3 class="comment-count">2 Comments</h3>
            <div class="comment-list">
              <div class="comment">
                <div class="comment-author-img">
                  <img
                    src="path-to-comment-author-image.jpg"
                    alt="comment author"
                  />
                </div>
                <div class="comment-content">
                  <h5 class="comment-author">Jane Smith</h5>
                  <span class="comment-date">June 25, 2021</span>
                  <p class="comment-text">
                    Ut enim ad minim veniam, quis nostrud exercitation ullamco
                    laboris nisi ut aliquip ex ea commodo consequat. Duis aute
                    irure dolor in reprehenderit in voluptate velit esse cillum
                    dolore eu fugiat nulla pariatur. Excepteur sint occaecat
                    cupidatat non proident, sunt in culpa qui officia deserunt
                    mollit anim id est laborum.
                  </p>
                </div>
              </div>
              <!-- end comment -->

              <div class="comment">
                <div class="comment-author-img">
                  <img
                    src="path-to-comment-author-image.jpg"
                    alt="comment author"
                  />
                </div>
                <div class="comment-content">
                  <h5 class="comment-author">John Doe</h5>
                  <span class="comment-date">June 28, 2021</span>
                  <p class="comment-text">
                    Duis aute irure dolor in reprehenderit in voluptate velit
                    esse cillum dolore eu fugiat nulla pariatur. Excepteur sint
                    occaecat cupidatat non proident, sunt in culpa qui officia
                    deserunt mollit anim id est laborum.
                  </p>
                </div>
              </div>
              <!-- end comment -->
            </div>
            <!-- end comment-list -->

            <div class="comment-form">
              <h3 class="comment-reply-title">Leave a Reply</h3>
              <form
                class="form-contact comment_form"
                action="#"
                method="post"
                id="commentForm"
              >
                <div class="row">
                  <div class="col-md-6">
                    <div class="form-group">
                      <input
                        type="text"
                        class="form-control"
                        id="name"
                        name="name"
                        placeholder="Name"
                        required
                      />
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <input
                        type="email"
                        class="form-control"
                        id="email"
                        name="email"
                        placeholder="Email"
                        required
                      />
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <input
                    type="text"
                    class="form-control"
                    id="website"
                    name="website"
                    placeholder="Website"
                    required
                  />
                </div>
                <div class="form-group">
                  <textarea
                    class="form-control"
                    name="message"
                    id="message"
                    rows="5"
                    placeholder="Message"
                    required
                  ></textarea>
                </div>
                <div class="form-group">
                  <button
                    type="submit"
                    class="btn btn-primary btn-round btn-lg"
                  >
                    Submit Comment
                  </button>
                </div>
              </form>
            </div>
            <!-- end comment-form -->
          </div>
          <!-- end comment-area -->
        </div>
        <!-- end col-lg-8 -->

        <div class="col-lg-4">
          <aside class="sidebar">
            <div class="widget widget-search">
              <form class="form-search">
                <input
                  type="text"
                  class="form-control"
                  placeholder="Search.."
                />
                <button class="btn" type="button">
                  <i class="fal fa-search"></i>
                </button>
              </form>
            </div>
            <!-- end widget-search -->

            <div class="widget widget-categories">
              <h5 class="widget-title">Categories</h5>
              <ul>
                <li><a href="#">Business</a></li>
                <li><a href="#">Technology</a></li>
                <li><a href="#">Health</a></li>
                <li><a href="#">Fashion</a></li>
                <li><a href="#">Travel</a></li>
              </ul>
            </div>
            <!-- end widget-categories -->

            <div class="widget widget-recent-posts">
              <h5 class="widget-title">Recent Posts</h5>
              <ul>
                <li><a href="#">Lorem Ipsum Dolor Sit Amet</a></li>
                <li><a href="#">Consectetur Adipisicing Elit</a></li>
                <li><a href="#">Sed Do Eiusmod Tempor</a></li>
                <li><a href="#">Incididunt Ut Labore Et Dolore</a></li>
                <li><a href="#">Magna Aliqua Ut Enim Ad Minim</a></li>
              </ul>
            </div>
            <!-- end widget-recent-posts -->

            <div class="widget widget-tags">
              <h5 class="widget-title">Tags</h5>
              <ul class="list-inline">
                <li><a href="#">Business</a></li>
                <li><a href="#">Technology</a></li>
                <li><a href="#">Health</a></li>
                <li><a href="#">Fashion</a></li>
                <li><a href="#">Travel</a></li>
              </ul>
            </div>
            <!-- end widget-tags -->
          </aside>
          <!-- end sidebar -->
        </div>
        <!-- end col-lg-4 -->
      </div>
      <!-- end row -->
    </div>
    <!-- end container -->
  </section>
  <!-- end blog-section -->
  {%-endfor-%}
</div>
<!-- end page-wrap -->
