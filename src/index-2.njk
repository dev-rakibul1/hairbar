---
title: home_2
layout: "base.njk"
---

<!--======================================
        START HERO
======================================-->
<section class="hero-section on-dark">
  {%- for home_2_award_winning in collections.home_2_award_winning -%}
  <div
    class="hero-item set-bg overlay"
    data-setbg=" {{ home_2_award_winning.data.image1 }}"
  >
    <div class="container">
      <div class="row">
        <div class="col-lg-10 mx-auto">
          <div class="hero-content text-center">
            <h1
              class="hero-title mb-4"
              data-aos="zoom-in"
              data-aos-delay="50"
              data-aos-duration="500"
            >
              {{ home_2_award_winning.data.title }}
            </h1>
            <p
              class="hero-text mx-auto"
              data-aos="zoom-in"
              data-aos-delay="150"
              data-aos-duration="700"
            >
              {{ home_2_award_winning.data.para1 }}
            </p>
            <div class="gap-2x"></div>
            <!-- end gap -->
            <ul
              class="hero-btns btns-group"
              data-aos="zoom-in"
              data-aos-delay="250"
              data-aos-duration="900"
            >
              <li>
                <a
                  href="contact.html"
                  class="btn btn-lg btn-round-none btn-primary"
                >
                  {{ home_2_award_winning.data.btn1 }}</a
                >
              </li>
              <li>
                <a
                  href="about-us.html"
                  class="btn btn-lg btn-round-none btn-outline-light"
                >
                  {{ home_2_award_winning.data.btn2 }}</a
                >
              </li>
            </ul>
          </div>
        </div>
        <!-- end col -->
      </div>
      <!-- end row -->
    </div>
    <!-- end container -->
    <div class="scroll-down-wrap">
      <i class="far fa-angle-down"></i>
    </div>
  </div>
  {%- endfor -%}
  <!-- end hero-item -->
</section>
<!-- end hero-section -->
<!--======================================
        END HERO
======================================-->

<!--======================================
        START ABOUT
 ======================================-->
<section class="about-section section-space bg-primary-100">
  <div class="container">
    <div class="row align-items-center about-content-wrap">
      <div class="col-lg-5">
        {%- for home_2_We_Know_Your_Style in
        collections.home_2_We_Know_Your_Style -%}

        <div class="section-content mb-5 mb-lg-0 about-content">
          <h1 class="section-title mb-3">
            {{ home_2_We_Know_Your_Style.data.title }}
          </h1>
          <p>
            {{ home_2_We_Know_Your_Style.data.para1 }}
          </p>
          <p>
            {{ home_2_We_Know_Your_Style.data.para2 }}
          </p>
          <div class="gap-1x"></div>
          <a href="about-us.html" class="btn btn-lg btn-round-none btn-primary">
            {{ home_2_We_Know_Your_Style.data.btn }}</a
          >
        </div>
        {%- endfor -%}
        <!-- end section-content -->
      </div>
      <!-- end col -->
      <div class="col-lg-7">
        {%- for home_2_We_Know_Your_Style_image in
        collections.home_2_We_Know_Your_Style_image -%}
        <div class="/img-box">
          <img
            src="{{ home_2_We_Know_Your_Style_image.data.image1 }}"
            alt="{{ home_2_We_Know_Your_Style_image.data.image1Alt }}"
            class="rounded"
          />
        </div>
        {%- endfor -%}
        <!-- end img-box -->
      </div>
      <!-- end col -->
      {%- for home_2_We_Know_Your_Style_image in
      collections.home_2_We_Know_Your_Style_image -%}
      <div class="float-img">
        <img
          src="{{ home_2_We_Know_Your_Style_image.data.image2 }}"
          alt="{{ home_2_We_Know_Your_Style_image.data.image2Alt }}"
          class="rounded"
        />
      </div>
      {%- endfor -%}
    </div>
    <!-- end row -->
  </div>
  <!-- end container -->
</section>
<!-- end about-section -->
<!--======================================
        END ABOUT
======================================-->

<!--======================================
        START SERVICE
 ======================================-->
<section class="service-section section--space">
  <div class="container">
    <div class="section-head text-center">
      {%- for home_2_What_We_Can_Do_Title in
      collections.home_2_What_We_Can_Do_Title -%}
      <h1 class="section-title">
        {{ home_2_What_We_Can_Do_Title.data.title }}
      </h1>
      {%- endfor -%}
    </div>
    <!-- end section-head -->
    <div class="row">
      {%- for home_2_What_We_Can_Do in collections.home_2_What_We_Can_Do -%}
      <div class="col-lg-3 col-md-4 col-sm-6">
        <div class="card card-s1 card-service-s1 text-center">
          <img
            src="{{ home_2_What_We_Can_Do.data.image1 }}"
            alt="{{ home_2_What_We_Can_Do.data.image1Alt }}"
            class="card-img"
          />
          <h4 class="card-title">{{ home_2_What_We_Can_Do.data.title }}</h4>
        </div>
        <!-- end card -->
      </div>
      {%- endfor -%}
    </div>
    <!-- end row -->
  </div>
  <!-- end container -->
</section>
<!-- end service-section -->
<!--======================================
        END SERVICE
======================================-->

<!--======================================
        START PRICING
 ======================================-->
<section
  class="pricing-section section-space set-bg overlay bg-fixed overflow-hidden"
  data-setbg="/img/hero-1.jpg"
>
  <div class="container">
    <div class="section-head text-center">
      <h1 class="section-title text-white">Our Pricing</h1>
    </div>
    <!-- end section-head -->
    <div class="pricing-wrap">
      <div class="row">
        {%-for pricing in collections.pricing-%}
        <div
          class="col-lg-6 pr-lg-4"
          data-aos="fade-in"
          data-aos-delay="50"
          data-aos-duration="800"
        >
          <div class="media media-s1 align-items-center">
            <img
              src="{{ pricing.data.image }}"
              class="media-img"
              alt="srvice image"
            />
            <div class="media-body">
              <div
                class="price-info d-flex align-items-center justify-content-between mb-1"
              >
                <h5 class="media-title">{{ pricing.data.pricingTitle }}</h5>
                <span class="price-divider"></span>
                <span class="media-subtitle">{{ pricing.data.price }}</span>
              </div>
              <p>{{ pricing.data.desc }}</p>
            </div>
          </div>
          <!-- end media -->
        </div>

        {%-endfor-%}
        <!-- end col -->
      </div>
      <!-- end row -->pricing_btn {%- for pricing_btn in collections.pricing_btn
      -%}
      <a href="booking.html" class="btn btn-lg btn-primary btn-block">{{
        pricing_btn.data.btn
      }}</a>
      {%- endfor -%}
    </div>
    <!-- end pricing-wrap -->
  </div>
  <!-- end container -->
</section>
<!-- end pricing-section -->
<!--======================================
        END PRICING
======================================-->

<!--======================================
        START TESTIMONIAL
======================================-->
<section class="testimonial-section text-center section-space-b">
  <div class="container">
    <div class="section--head">
      <h1 class="section-title">Our Top Client Says</h1>
    </div>
    <!-- end section-head -->
    <div class="testimonial-slider owl-carousel owl-theme">
      {%-for testimonial in collections.testimonial-%}

      <div class="testimonial-item">
        <p class="lead mb-4">{{ testimonial.data.desc }}</p>
        <div class="avatar mx-auto mb-3">
          <img src="{{ testimonial.data.image }}" alt="" />
        </div>
        <h5 class="testimonial-title">
          {{ testimonial.data.testimonialTitle }}
        </h5>
      </div>
      {%-endfor-%}
    </div>
  </div>
  <!-- end container -->
</section>
<!--======================================
        END TESTIMONIAL
======================================-->

<!--======================================
        START TEAM
======================================-->
<section class="team-section section--space-b">
  <div class="container">
    <div class="section-head text-center">
      <h1 class="section-title">Our Barbers</h1>
    </div>
    <!-- end section-head -->
    <div class="row">
      {%-for team in collections.team-%}
      <div class="col-lg-4 col-sm-6">
        <div
          class="card card-s1 card-team text-center"
          data-aos="zoom-out"
          data-aos-delay="50"
          data-aos-duration="500"
        >
          <div class="card-image">
            <img
              src="{{ team.data.image }}"
              alt="team image"
              class="card-img-top"
            />
          </div>
          <div class="card-body">
            <h4 class="card-title mb-1">{{ team.data.name }}</h4>
            <p class="card-text">{{ team.data.profession }}</p>
          </div>
        </div>
        <!-- end card -->
      </div>
      {%-endfor-%}
      <!-- end col -->
    </div>
    <!-- end row -->
  </div>
  <!-- end container -->
</section>
<!-- end team-section -->
<!--======================================
        END TEAM
======================================-->

<!--======================================
        START PACKAGE
======================================-->
<section class="package-section section--space bg-primary-100">
  <div class="container">
    <div class="section-head text-center">
      <h1 class="section-title">Choose Your Package</h1>
    </div>
    <!-- end section-head -->
    <div class="row">
      {%-for home_2_package in collections.home_2_package-%}
      <div class="col-lg-4 col-md-6">
        <div class="card card-s1 card-price text-center">
          <div class="card-top">
            <h4 class="mb-2 top-title">{{ home_2_package.data.title }}</h4>
            <p class="top-desc">{{ home_2_package.data.meta }}</p>
          </div>
          <div class="single-price">
            <h1 class="price-title">{{ home_2_package.data.price }}</h1>
          </div>
          <ul class="list-item price-list">
            <li>{{ home_2_package.data.pack_ann1 }}</li>
            <li>{{ home_2_package.data.pack_ann2 }}li></li>
            <li>{{ home_2_package.data.pack_ann3 }}</li>
            <li>{{ home_2_package.data.pack_ann4 }}</li>
            <li>{{ home_2_package.data.pack_ann5 }}li></li>
          </ul>

          <div class="price-btn-wrap">
            <a href="#" class="btn btn-md btn-primary btn-block btn-upper">{{
              home_2_package.data.btn
            }}</a>
          </div>
        </div>
        <!-- end card -->
      </div>
      {%-endfor -%}
      <!-- end col -->
    </div>
    <!-- end row -->
  </div>
  <!-- end container -->
</section>
<!-- end package-section -->
<!--======================================
        END PACKAGE
======================================-->

<!--======================================
        START BLOG
======================================-->
<section class="blog-section section--space">
  <div class="container">
    <div class="section-head text-center">
      <h1 class="section-title">Our Latest News</h1>
    </div>
    <!-- end section-head -->
    <div class="row">
      {%-for home_2_blog in collections.home_2_blog-%}

      <div class="col-lg-6">
        <div class="card card-s1 card-flex">
          <a href="" class="card-image">
            <img
              src="{{ home_2_blog.data.image1 }}"
              alt="blog image"
              class="card-img"
            />
          </a>
          <div class="card-body card-body-s1">
            <ul class="blog-meta">
              <li>
                <a href="#"
                  ><i class="far icon fa-calendar"></i
                  >{{ home_2_blog.data.time }}</a
                >
              </li>
              <li>
                <a href="#"
                  ><i class="far icon fa-heart"></i
                  >{{ home_2_blog.data.like }}</a
                >
              </li>
              <li>
                <a href="#"
                  ><i class="far icon fa-comment"></i
                  >{{ home_2_blog.data.comments }}</a
                >
              </li>
            </ul>
            <h5 class="card-title my-2">
              <a href="blog-details.html">{{ home_2_blog.data.title }}</a>
            </h5>
            <p class="card-text">
              {{ home_2_blog.data.para1 }}
            </p>
          </div>
        </div>
      </div>
      {%-endfor-%}
    </div>
    <!-- end row -->
  </div>
  <!-- end container -->
</section>
<!-- end blog-section -->
<!--======================================
        END BLOG
======================================-->
